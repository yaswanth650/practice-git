prefix=/opt/inspec/embedded
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: OpenSSL-libssl
Description: Secure Sockets Layer and cryptography libraries
Version: 1.0.2r
Requires.private: libcrypto
Libs: -L${libdir} -lssl
Libs.private: -Wl,-rpath,/opt/inspec/embedded/lib -L/opt/inspec/embedded/lib -ldl
Cflags: -I${includedir} 
